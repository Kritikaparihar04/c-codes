int firstIndex(int* input,int n,int x) {
  
 if(n==0)
     return -1; //base case
 
    if(input[0]==x)
        return 0 ;
    
    int sm=firstIndex(input+1,n-1,x);
    if(sm==-1)
        return -1;
    else
    return sm+1;
    
}
#include<iostream>
#include "Solution.h"
using namespace std;


int main(){
    int n;
    cin >> n;
  
    int *input = new int[n];
    
    for(int i = 0; i < n; i++) {
        cin >> input[i];
    }
    
    int x;
    
    cin >> x;
    
    cout << firstIndex(input, n, x) << endl;

}


