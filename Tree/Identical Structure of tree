
bool areIdentical(TreeNode<int> *root1, TreeNode<int> * root2)
{
    
    if(root1==NULL|| root2==NULL)
        return  NULL;
    
    //sare false vali comditiom likhdo
    
    if(root1->data !=root2->data)
    {
        return false;
    }
    
    for(int i=0;i<root1->children.size();i++)
        
    {
        bool Is_bl=areIdentical(root1->children[i],root2->children[i]);
        
        
        if(!Is_bl)
            return false;
    }
    
    return true;
    
    
    
    
}

