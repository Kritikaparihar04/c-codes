 

int lastIndex(int input[], int size, int x) {
 
    
    for(int i=size-1;i>=0;i--)
    {
        if(input[i]==x)
            return i;
    }
    return -1;
    
}

#include<iostream>
#include "Solution.h"
using namespace std;


int main(){
    int n;
    cin >> n;
  
    int *input = new int[n];
    
    for(int i = 0; i < n; i++) {
        cin >> input[i];
    }
    
    int x;
    
    cin >> x;
    
    cout << lastIndex(input, n, x) << endl;

}





