#include <iostream>

using namespace std;

int main()
{
   cout << "BITWISE OPERATOR" << endl; 
   int ror=15|30;
   int r_and=15&30;
   int rnot=~15;
   int rxor=15^30;
   int rrs=15>>2;
   int rls=15<<2;
   cout<<ror<<endl;
   
   cout<<r_and<<endl;
   cout<<rnot<<endl;
   cout<<rxor<<endl;
   cout<<rrs<<endl;
   cout<<rls<<endl;
   return 0;
}
