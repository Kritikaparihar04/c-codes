void insertionSort2(int n, vector<int> arr) {

for(int i=1;i<n;i++)
{
    if(arr[i]<arr[i-1])
    {
        
        for(int j=i;j>=0;j--)
        {
            if(arr[j]<arr[j-1])
            {
                swap(arr[j],arr[j-1]);
            }
            else {
            break;
            }
        }
    }
    
    for(int k=0;k<n;k++)
    {
        cout<<arr[k]<<" ";
    }
    
    cout<<endl;
    
}

}
