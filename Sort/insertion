void insertionSort1(int n, vector<int> arr) {

int temp=arr[n-1];

for(int i=n-2;i>=-1;i--)
{
    if(arr[i]>temp)
    {
        arr[i+1]=arr[i];
    }
    else
    {
        arr[i+1]=temp;
        temp=-1;
    }
    
    for(int k=0;k<n;k++)
    {
        cout<<arr[k]<<" ";
    }
    cout<<endl;
    if(temp==-1)
      break;
}



}
