#include <iostream>
using namespace std;
#include <bits/stdc++.h>
int main()
{
    string s = "gkhd";
    string t = "lgkhdfgt";
    int m = s.size();
    int n = t.size();
    int **output = new int *[m + 1];
    for (int i = 0; i <= m; i++)
    {
        output[i] = new int[n + 1];
    }

   
    for (int i = 0; i <= m; i++)
    {
        for (int j = 0; j <= n; j++)
        {
            if (i == 0 || j == 0)
            {
                output[i][j] = 0;
            }
            else if (s[m-i] == t[n-j])            //m-i because we havr to match the last characters ,,only i will means the starting characters
            {
                output[i][j] = 1 + max(output[i - 1][j - 1], max(output[i][j - 1], output[i - 1][j]));
             
            }
            else
            {
                output[i][j] = max(output[i - 1][j - 1], max(output[i][j - 1], output[i - 1][j]));
            }
        }
    }
    cout<<output[m][n];
}
